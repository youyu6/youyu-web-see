{
    "compilerOptions": {
      "target": "es2018",
      "module": "esnext",
      "lib": ["esnext", "dom"],
      "declaration": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true
    },
}